{
  "name": "phone-tracker",
  "version": "1.0.0",
  "description": "A website for tracking missing cellphones using phone numbers, IMEI, or email",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build complete - static files ready'",
    "serve": "npx serve public -p 8000",
    "deploy:netlify": "netlify deploy --prod --dir=public",
    "deploy:vercel": "vercel --prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "echo 'No linting configured yet'",
    "preview": "npx serve public -p 3000"
  },
  "keywords": [
    "phone",
    "tracker",
    "lost",
    "cellphone",
    "IMEI",
    "tracking"
  ],
  "author": "Phone Tracker Team",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "sqlite3": "^5.1.6",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/phone-tracking-website.git"
  },
  "homepage": "https://yourusername.github.io/phone-tracking-website",
  "bugs": {
    "url": "https://github.com/yourusername/phone-tracking-website/issues"
  }
}
